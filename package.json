{
  "name": "auth",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "watch": "npm run build -- -w",
    "start:ios": "npm run build && concurrently -r 'npm run watch' 'react-native run-ios'",
    "start:android": "npm run build && concurrently -r 'npm run watch' 'react-native run-android'",
    "tsc": "tsc",
    "lint": "tslint src/**/*.ts",
    "clean": "rimraf artifacts",
    "build": "npm run clean && npm run tsc --",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest"
  },
  "dependencies": {
    "firebase": "^3.9.0",
    "lodash": "^4.17.4",
    "react": "16.0.0-alpha.6",
    "react-native": "0.44.0",
    "react-native-communications": "^2.2.1",
    "react-native-google-places-autocomplete": "^1.2.12",
    "react-native-maps": "^0.15.2",
    "react-native-material-kit": "^0.4.1",
    "react-native-material-ui": "^1.12.0",
    "react-native-router-flux": "^3.40.1",
    "react-native-vector-icons": "^4.2.0",
    "react-redux": "^5.0.5",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "@types/jest": "^20.0.2",
    "@types/react": "^15.0.33",
    "@types/react-native": "^0.44.14",
    "babel-jest": "20.0.1",
    "babel-preset-react-native": "1.9.2",
    "concurrently": "^3.5.0",
    "eslint-config-rallycoding": "^3.2.0",
    "jest": "20.0.1",
    "react-test-renderer": "16.0.0-alpha.6",
    "rimraf": "^2.6.1",
    "tslint": "^5.4.3",
    "typescript": "^2.4.1"
  },
  "jest": {
    "preset": "react-native"
  }
}
